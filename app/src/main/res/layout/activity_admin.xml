<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.missgumus.mobile.missgumus5.AdminActivity">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="0dp"
        android:layout_height="105dp"
        android:layout_marginTop="84dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/button11"
        android:layout_width="157dp"
        android:layout_height="47dp"
        android:layout_marginTop="84dp"
        android:background="@color/colorPrimaryDark"
        android:onClick="urunEkle"
        android:text="Urun Ekle"
        android:textColor="@color/backgroundColor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <Button
        android:id="@+id/button12"
        android:layout_width="157dp"
        android:layout_height="47dp"
        android:layout_marginTop="20dp"
        android:background="@color/colorPrimaryDark"
        android:text="ürün sil "
        android:textColor="@color/backgroundColor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button11" />

    <Button
        android:id="@+id/button13"
        android:layout_width="157dp"
        android:layout_height="47dp"
        android:layout_marginTop="20dp"
        android:background="@color/colorPrimaryDark"
        android:text="ürün güncelle"
        android:textColor="@color/backgroundColor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button12" />

    <Button
        android:id="@+id/button14"
        android:layout_width="157dp"
        android:layout_height="47dp"
        android:layout_marginTop="24dp"
        android:background="@color/colorPrimaryDark"
        android:onClick="kargoGir"
        android:text="Kargo No Gir"
        android:textColor="@color/backgroundColor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button13" />


    <include
        android:id="@+id/menu"
        layout="@layout/activity_menubar"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</android.support.constraint.ConstraintLayout>
